//write a program to get a number from the total number of digits in that number
#include <stdio.h>

int main() {
    int n, c=0;
    scanf("%d", &n);
    if (n == 0) {
        c=1;
        goto END;
    }

START:
    if (n == 0)
        goto END;

    c++;
    n = n / 10;
    goto START;

END:
    printf("%d", c);
    return 0;
}
