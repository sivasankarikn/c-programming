//write a program to get a number from the user and print the sum of all digits
#include <stdio.h>

int main() {
    int n, c=0,a;
    scanf("%d", &n);
    if (n == 0) {
        goto END;
    }

START:
    if (n == 0)
        goto END;

    
    a= n %10;
    c+=a;
    n=n/10;
    goto START;

END:
    printf("%d", c);
    return 0;
}

