//Get a four-digit number from the user and check if the first two digits and last two digits are the same. If yes, print "Yes", otherwise print "Failure".
#include <stdio.h>
int main() {
    int n,a,b;
    scanf("%d",&n);
    a=n%100;
    b=n/100;
    if(a==b) 
    printf("success");
    else
    printf("failure");
    return 0;
}
