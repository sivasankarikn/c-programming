//get a four-digit number from the user and check if the digit 1 and digit 0 are the same, and if digit 1 and digit 0 are different. If yes, print success, otherwise print failure.
#include <stdio.h>
int main() {
    int n,a,b;
    scanf("%d",&n);
    a=n%10;
    b=n/10;
    if(a==b) 
    printf("success");
    else
    printf("failure");
    return 0;
}
